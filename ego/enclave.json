{
    "exe": "gaiko-ego",
    "key": "private.pem",
    "debug": true,
    "heapSize": 16384,
    "executableHeap": false,
    "productID": 1,
    "securityVersion": 1,
    "mounts": [
        {
            "source": "/root/.config/raiko/config",
            "target": "/root/.config/raiko/config",
            "type": "hostfs",
            "readOnly": false
        },
        {
            "source": "/root/.config/raiko/secrets",
            "target": "/root/.config/raiko/secrets",
            "type": "hostfs",
            "readOnly": false
        },
        {
            "source": "/proc/self/mountinfo",
            "target": "/proc/self/mountinfo",
            "type": "hostfs",
            "readOnly": true
        },
        {
            "source": "/proc/self/cgroup",
            "target": "/proc/self/cgroup",
            "type": "hostfs",
            "readOnly": true
        },
        {
            "source": "/sys/fs/cgroup/",
            "target": "/sys/fs/cgroup/",
            "type": "hostfs",
            "readOnly": true
        }
    ],
    "env": [
        {
            "name": "SGX_TYPE",
            "value": "ego"
        },
        {
            "name": "GAIKO",
            "value": "1"
        },
        {
            "name": "HOME",
            "value": "/root"
        },
        {
            "name": "GODEBUG",
            "value": "madvdontneed=1"
        }
    ],
    "files": []
}
